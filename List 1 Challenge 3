names = []
names.append(input("Enter name1 >>>"))
names.append(input("Enter name2 >>>"))
names.append(input("Enter name3 >>>"))

people = (input("Would you like to invite another person? Y or N"))

while people == "Y":
  names.append(input("Enter a name. N to exit"))

if people == "N":
  print(names)

elif names == "N":
  print(names)
  
else:
  print(names)
